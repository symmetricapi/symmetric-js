{
  "name": "symmetric-js",
  "version": "1.0.15",
  "description": "Symmetric provides observable model and collection classes with support for field specifications that mirror backend sources with encoding functions and validation rules.",
  "keywords": [
    "react",
    "observable",
    "model",
    "collection",
    "rest",
    "api",
    "fetch",
    "backbone",
    "symmetric"
  ],
  "main": "dist/symmetric.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "release": "yarn build && git tag $npm_package_version && git push --tags && yarn publish --no-lockfile",
    "test": "node test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:symmetricapi/symmetric-js.git"
  },
  "homepage": "https://github.com/symmetricapi/symmetric-js",
  "bugs": {
    "url": "https://github.com/symmetricapi/symmetric-js/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "node-fetch": "^3.1.1",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100,
    "arrowParens": "avoid"
  }
}
